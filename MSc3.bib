issn = {0028-0836},
journal = {Nature},
keywords = {cancer,inflammation},
mendeley-tags = {cancer,inflammation},
pages = {461--466},
pmid = {15329734},
title = {{NF-kappaB functions as a tumour promoter in inflammation-associated cancer.}},
volume = {431},
year = {2004}
}
